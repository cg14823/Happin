<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="mosaic.happin">

<application android:allowBackup="true" android:icon="@mipmap/ic_launcher" android:label="@string/app_name"
    android:supportsRtl="true" android:theme="@style/AppTheme">
    <activity android:name=".Login">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:name="mosaic.happin.MainActivity" android:label="@string/title_activity_main"
        android:theme="@style/AppTheme" android:parentActivityName="mosaic.happin.Login">
        <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="mosaic.happin.Login" />
    </activity>
</application>

</manifest>

